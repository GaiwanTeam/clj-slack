{:paths ["src" "resources"]
 :deps
 {org.clojure/clojure {:mvn/version "1.11.1"}

  ;; Data formats
  metosin/jsonista      {:mvn/version "0.3.6"}
  lambdaisland/uri      {:mvn/version "1.13.95"}
  com.cnuernber/charred {:mvn/version "1.011"}

  ;; Time
  clojure.java-time/clojure.java-time {:mvn/version "0.3.3"}

  ;; Http/WS
  hato/hato                         {:mvn/version "0.8.2"}
  org.java-websocket/Java-WebSocket {:mvn/version "1.5.3"}

  ;; Logging
  io.pedestal/pedestal.log {:mvn/version "0.5.10"}
  com.lambdaisland/glogi   {:mvn/version "1.1.144"}

  co.gaiwan/data-tools {:git/url "git@github.com:GaiwanTeam/data-tools"
                        :git/sha "f00918bd6cd3d6e46276b16dd9c2f061eec76bba"}

  org.slf4j/jul-to-slf4j         {:mvn/version "1.7.36"}
  org.slf4j/jcl-over-slf4j       {:mvn/version "1.7.36"}
  org.slf4j/log4j-over-slf4j     {:exclusions [org.slf4j/slf4j-nop], :mvn/version "1.7.36"}
  ch.qos.logback/logback-classic {:exclusions [org.slf4j/slf4j-api org.slf4j/slf4j-nop] :mvn/version "1.2.11"}}

 :aliases
 {:dev  {:extra-paths ["dev"]
         :extra-deps  {com.lambdaisland/classpath  {:mvn/version "RELEASE"}
                       io.github.nextjournal/clerk {:mvn/version "RELEASE"}
                       djblue/portal               {:mvn/version "RELEASE"}}}
  :test {:extra-paths ["test"]
         :extra-deps  {lambdaisland/kaocha {:mvn/version "RELEASE"}}}

  :garden {:extra-deps {garden/garden {:mvn/version "1.3.10"}}}

  :run/backfill {:exec-fn co.gaiwan.slack.backfill/backfill}}}
